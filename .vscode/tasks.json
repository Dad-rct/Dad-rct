{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "webpack web producer",
			"group": "build",
			"isBackground": true,
			"command": "./node_modules/.bin/webpack",
			"options": {
				"cwd": "${workspaceFolder}/client/",
				"env": {
					"out": "producer",
					"target": "web"
				}
			},
			"problemMatcher": {
				"owner": "webpack",
				"severity": "error",
				"fileLocation": "relative",
				"pattern": [
					{
						"regexp": "ERROR in (.*)",
						"file": 1
					},
					{
						"regexp": "\\((\\d+),(\\d+)\\):(.*)",
						"line": 1,
						"column": 2,
						"message": 3
					}
				]
			}
		}
	]
}